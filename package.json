{
    "name": "gatsby-remark-extract-keywords",
    "description": "Extract keywords (for SEO, display, etc) on all remark nodes.",
    "version": "0.0.1",
    "author": "Eduardo Reveles <me@osiux.ws>",
    "license": "MIT",
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "remark",
        "keywords"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/osiux/gatsby-remark-extract-keywords"
    },
    "homepage": "https://github.com/osiux/gatsby-remark-extract-keywords#readme",
    "scripts": {
        "prebuild": "yarn test",
        "build": "cross-env BABEL_ENV=production babel src --out-dir .",
        "test": "yarn test:lint && yarn test:prettier",
        "test:lint": "eslint --ext .js src",
        "test:prettier": "yarn prettier --list-different",
        "prepublishOnly": "yarn build",
        "prettier": "prettier \"**/*.{js,json,md}\"",
        "format": "yarn prettier --write"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "nlcst-to-string": "^2.0.3",
        "retext": "^7.0.1",
        "retext-keywords": "^5.0.0",
        "retext-pos": "^2.0.2"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^5.0.0",
        "@semantic-release/git": "^9.0.0",
        "babel-cli": "^6.26.0",
        "babel-eslint": "^9.0.0",
        "babel-preset-env": "^1.7.0",
        "cross-env": "^5.1.5",
        "eslint": "^5.3.0",
        "eslint-config-prettier": "^3.0.1",
        "husky": "^1.0.0-rc.13",
        "lint-staged": "^7.2.2",
        "prettier": "1.14.2"
    },
    "peerDependencies": {
        "gatsby": "^2.0.0"
    }
}
